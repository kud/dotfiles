#! /usr/bin/env zsh

source $MY/core/helper

# install python packages
echo_space
echo_title_install "python @ work"

# Contexte
## Journal
CTX_JOURNAL_PYTHON_VERSION="2.7.10"
pyenv install -s $CTX_JOURNAL_PYTHON_VERSION
pyenv virtualenv $CTX_JOURNAL_PYTHON_VERSION contexte

## Data
CTX_DATA_PYTHON_VERSION="2.7.10"
pyenv install -s $CTX_DATA_PYTHON_VERSION
pyenv virtualenv $CTX_DATA_PYTHON_VERSION lois-webapp

